
.toast-content-wrap {
    display: flex;
    position: fixed;
    height: 100%;
    width: 100%;
    justify-content: center;
    align-items: center;
    z-index: 999;
}
 
.toast-content {
    height: 196rpx;
    width: 360rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    text-align: center;
    color: #d2d2d2;
    background: rgba(0, 0, 0, 0.90);
    border-radius: 8px;
}
